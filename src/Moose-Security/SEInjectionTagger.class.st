Class {
	#name : 'SEInjectionTagger',
	#superclass : 'SEVulnerabilityTagger',
	#category : 'Moose-Security-Taggers',
	#package : 'Moose-Security',
	#tag : 'Taggers'
}

{ #category : 'default taggers' }
SEInjectionTagger class >> addMethodNameTaggersTo: taggers [

	<cautionTagger>
	#( #executeQuery #getConnection ) do: [ :methodName |
		taggers add: (self tagResultOf: [ :model :tagger |
				 tagger methodsNamed: methodName in: model ]) ]
]

{ #category : 'accessing' }
SEInjectionTagger class >> tagColor [
	"Method generated by SEVulnerabilityTagger class>>#generateColorMethods"

	^ (Color r: 0.38514173998044965 g: 0.6999022482893451 b: 0.07038123167155426 alpha: 1.0)
]

{ #category : 'accessing' }
SEInjectionTagger class >> tagName [

	^ #Injection
]
